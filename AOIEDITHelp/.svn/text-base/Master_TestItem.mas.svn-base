    <test_instance>
        <test testid="圆心定位">
            <subtest subtestid="圆心查找算法1">
                <exe>
                    <exename>CenterDll</exename>
                    <exeversion>1.0.1.0</exeversion>
                </exe>
                <order>999</order>
                <runtest>NO</runtest>
                <parameter name="Thred_Center1" parameterDesc="检测阀值">
                    <property>
                        <interface>iint</interface>
                        <visibility>GURU</visibility>
                        <max>100</max>
                        <min>5</min>
                    </property>
                    <setting>15</setting>
                    <helpdescription>圆心查找第一次种方法的参数值, 大小表示灰度的差值. 如果背景亮此值见意大点</helpdescription>
                </parameter>
                <parameter name="Thred_Center2" parameterDesc="检测阀值2">
                    <property>
                        <interface>ifloat</interface>
                        <max>120</max>
                        <min>5</min>
                        <visibility>GURU</visibility>
                    </property>
                    <setting>50</setting>
                    <helpdescription>圆心查找第一次种方法的参数值, 大小表示灰度的差值. 如果背景亮此值见意大点. 此值是对第一种处理结果不理想的状况下才会被运行</helpdescription>
                </parameter>
                <parameter name="XMax" parameterDesc="水平位置最大值">
                    <property>
                        <interface>iint</interface>
                        <max>550</max>
                        <min>200</min>
                    </property>
                    <setting>400</setting>
                    <helpdescription>圆心X轴方向上X的最大值</helpdescription>
                </parameter>
                <parameter name="XMin" parameterDesc="水平位置最小值">
                    <property>
                        <interface>iint</interface>
                        <max>500</max>
                        <min>50</min>
                    </property>
                    <setting>100</setting>
                    <helpdescription>圆心X轴方向上X的最小值</helpdescription>
                </parameter>
                <parameter name="YMax" parameterDesc="垂直位置最大值">
                    <property>
                        <interface>iint</interface>
                        <max>500</max>
                        <min>50</min>
                    </property>
                    <setting>400</setting>
                    <helpdescription>圆心Y轴方向上Y的最大值</helpdescription>
                </parameter>
                <parameter name="YMin" parameterDesc="垂直位置最小值">
                    <property>
                        <interface>iint</interface>
                        <max>500</max>
                        <min>5</min>
                    </property>
                    <setting>50</setting>
                    <helpdescription>圆心Y轴方向上Y的最小值</helpdescription>
                </parameter>
                <parameter name="RMax" parameterDesc="检测圆心半径最大值">
                    <property>
                        <interface>iint</interface>
                        <max>500</max>
                        <min>80</min>
                    </property>
                    <setting>375</setting>
                    <helpdescription>圆心半径容许的最大值</helpdescription>
                </parameter>
                <parameter name="RMin" parameterDesc="检测圆半径最小值">
                    <property>
                        <interface>iint</interface>
                        <max>200</max>
                        <min>60</min>
                    </property>
                    <setting>80</setting>
                    <helpdescription>圆心半径容许的最小值</helpdescription>
                </parameter>
                <property>
                    <interface>common</interface>
                </property>
                <helpdescription>圆心查找算法1. 主要是按灰度进行一些圆心的查找定位</helpdescription>
            </subtest>
